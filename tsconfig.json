{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "baseUrl": "./src",
    "rootDir": "./src",
    "outDir": "./dist",
    "noImplicitAny": false,
    "allowUnreachableCode": true,
    "esModuleInterop": true,
    "allowJs": true,
  },
  "include": [
    "src/**/*.ts",
    "test/**/*.ts"
  ]
}
